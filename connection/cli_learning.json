{
    "cisco": [
        "vlan 10",
        "interface GigabitEthernet1/0/1",
        "ip address 192.168.1.1 255.255.255.0"
    ],
    "juniper": [
        "set vlans VLAN-10 vlan-id 10",
        "set interfaces ge-0/0/1 unit 0 family inet address 192.168.1.1/24"
    ],
    "hp": [
        "vlan 10",
        "interface 1",
        "ip address 192.168.1.1/24"
    ],
    "arista": [
        "vlan 10",
        "interface Ethernet1",
        "ip address 192.168.1.1/24"
    ],
    "coreedge": [
        "create vlan 10 name VLAN_10",
        "interface 1 set vlan 10"
    ],
    "handreamnet": [
        "vlan database",
        "vlan 10",
        "interface 1 switchport access vlan 10"
    ]
}